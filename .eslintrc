{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "rules": {
    "indent": [2, 2, { "SwitchCase": 1 }],
    "no-use-before-define": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "graphql/template-strings": ['error', {
      env: 'literal'
    }]
  },
  "globals": {
    "expect": true,
    "sinon": true
  },
  plugins: [
    'graphql'
  ]
}
