{
  "name": "ClientRelationshipManagerUI",
  "version": "1.0.0",
  "description": "Web client for CRM graphql api",
  "main": "index.html",
  "scripts": {
    "start": "npm run build && node server.js",
    "start:only-server": "node server.js",
    "build": "rm -rf dist && webpack --config webpack/webpack.config.prod.js --progress --profile --colors",
    "dev": "webpack-dashboard -- webpack-dev-server --open --config webpack/webpack.config.dev.js --progress",
    "stage": "NODE_ENV=staging webpack-dashboard -- webpack-dev-server --open --config webpack/webpack.config.dev.js --progress",
    "test": "NODE_ENV=testing mocha --opts ./mocha.opts ./src/**/*.spec.js",
    "test:watch": "NODE_ENV=testing mocha --watch --opts ./mocha.opts ./src/**/*.spec.js",
    "test-features": "babel-node ./node_modules/codeceptjs/bin/codecept.js run --steps",
    "test-features:selenium-install": "./node_modules/.bin/selenium-standalone install",
    "test-features:selenium-start": "./node_modules/.bin/selenium-standalone start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VasilyShelkov/ClientRelationshipManagerUI.git"
  },
  "keywords": [
    "node",
    "react",
    "express",
    "hot-loading",
    "webpack-hot-middleware",
    "babel6",
    "eslint"
  ],
  "author": "Vasily Shelkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VasilyShelkov/ClientRelationshipManagerUI/issues"
  },
  "homepage": "https://github.com/VasilyShelkov/ClientRelationshipManagerUI#readme",
  "dependencies": {
    "apollo-client": "^0.5.19",
    "axios": "^0.15.3",
    "babel-core": "^6.20.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "graphql-tag": "^1.2.0",
    "html-webpack-plugin": "^2.24.1",
    "js-cookie": "^2.1.3",
    "material-ui": "^0.16.5",
    "moment": "^2.17.1",
    "node-sass": "^3.13.1",
    "places.js": "^1.4.8",
    "postcss-loader": "^1.2.0",
    "react": "^15.4.1",
    "react-apollo": "^0.7.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-form": "^6.3.2",
    "redux-form-material-ui": "^4.1.2",
    "redux-localstorage": "^1.0.0-rc5",
    "redux-localstorage-filter": "^0.1.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.27"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codeceptjs": "^0.4.13",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-graphql": "^0.4.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extend": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "mocha-graphql-register": "^1.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "selenium-standalone": "^5.9.1",
    "selenium-webdriver": "^3.0.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "webdriverio": "^4.6.1",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.1.0-beta.12"
  }
}
